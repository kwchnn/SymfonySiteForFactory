<!DOCTYPE html>

<title>Item</title>

{% block body %}
    <h1>Item</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ item.id }}</td>
            </tr>
            <tr>
                <th>ItemCategoryId</th>
                <td>{{ item.itemCategoryId }}</td>
            </tr>
            <tr>
                <th>Sort</th>
                <td>{{ item.sort }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ item.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ item.description }}</td>
            </tr>
            <tr>
                <th>Params</th>
                <td>{{ item.params }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ item.price }}</td>
            </tr>
            <tr>
                <th>SeoTitle</th>
                <td>{{ item.seoTitle }}</td>
            </tr>
            <tr>
                <th>SeoDescriptions</th>
                <td>{{ item.seoDescriptions }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_item_index') }}">back to list</a>

    <a href="{{ path('app_item_edit', {'id': item.id}) }}">edit</a>

    {{ include('item/_delete_form.html.twig') }}
{% endblock %}
